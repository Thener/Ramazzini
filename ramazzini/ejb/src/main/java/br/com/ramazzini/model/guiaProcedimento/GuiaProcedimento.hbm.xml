<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
		"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>

	<query name="GuiaProcedimento.recuperarPorGuia">
		<![CDATA[
			select 
				gp
			from 
				GuiaProcedimento gp
			where 
				gp.guia = :guia
			order 
				by gp.id
		]]>
	</query>	

</hibernate-mapping>